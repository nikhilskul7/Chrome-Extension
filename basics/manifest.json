{
    "manifest_version":2,
    "name":"Nikhil's Extension",
    "version":"1",
    "content_scripts":
    [
        {
            "matches":
            [
                "<all_urls>"
            ],
            "js":
            [
              "word.js"
            ]
        }

    ],
    "browser_action":{
        "default_icon":"1.png",
        "default_popup":"popup.html",
        "default_title":"New Word"
        
    }
    

}